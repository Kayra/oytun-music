option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: oytun_music/oytun_music.settings
    PYTHONPATH: /var/app/current:$PYTHONPATH
  aws:elasticbeanstalk:container:python:
    WSGIPath: oytun_music.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static

container_commands:
  01_collectstatic:
    command: "source /var/app/venv/*/bin/activate && python oytun_music/manage.py collectstatic --noinput"
    leader_only: true